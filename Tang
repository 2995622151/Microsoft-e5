int main()  //求所有数的合
{
	int i, a[10], sum = 0;
	for (i = 0; i < 10; i++)
	{
		scanf("%d", &a[i]);
	}
	for (i = 0; i < 10; i++)
	{
		sum = sum + a[i];
	}
	printf("%d\n", sum);
	return 0;
}
*/

int main()   //求平均数 
{
	int i, sum = 0, a[10];
	double pinjun;
	for (i = 0; i < 10; i++) 
	{
		scanf("%d", &a[i]);
	}
	for (i = 0; i < 10; i++)
	{
		sum += a[i];
	}
	pinjun = (double)sum / 10;
	printf("%f\n", pinjun);
	return 0;
}
